{"ast":null,"code":"import axios from \"axios\";\n//import PaginationComponent from './layout/PaginationComponent.vue';\n// import {plusCalculator} from './common.js'; //reactive 추가\nexport default {\n  //components: { PaginationComponent },\n  name: 'mainComponent',\n  data() {\n    return {\n      list: [],\n      searchKey: '',\n      searchValue: ''\n    };\n  },\n  methods: {\n    async search() {\n      try {\n        var searchKey = this.$refs.searchForm.searchKey.value;\n        var searchValue = this.$refs.searchForm.searchValue.value;\n        if (searchValue == \"\") {\n          searchValue = null;\n          searchKey = null;\n        }\n        // console.log(\"searchKey...?\"+searchKey);\n        // console.log(\"searchValue...?\"+searchValue);\n        const res = await axios.get(\"http://localhost:6005/freeBoardList/\" + searchKey + \"/\" + searchValue);\n        //console.log(res)\n\n        this.list = res.data.list;\n      } catch (error) {\n        console.log(error);\n      }\n    }\n  },\n  mounted() {\n    //console.log(\"mount 되고나서 search()함수를 탄다\")\n    console.log(this.$route);\n    this.search();\n  }\n};","map":{"version":3,"names":["axios","name","data","list","searchKey","searchValue","methods","search","$refs","searchForm","value","res","get","error","console","log","mounted","$route"],"sources":["C:\\practice\\Practice_Vue.js\\maven_board_vue\\src\\views\\MainComponent.vue"],"sourcesContent":["<template id=\"mainComponent\">\n  <section>\n    <div style=\"background-color:#dedede; height:700px;\">\n      <div>\n        <h1>자유게시판</h1>\n      </div>\n      <div style=\"width:650px;\" align=\"right\">\n        <router-link to=\"/regPost\">글쓰기</router-link>\n      </div>\n      <hr style=\"width: 600px\">\n      <div v-for=\"item of list\" :key=\"item.id\"> \n        <div style=\"width: 50px; float: left;\" v-text=\"item.num\"></div>\n        <div style=\"width: 300px; float: left;\"><router-link :to=\"{ name: 'DetailPost', params: {seq: item.num} }\">{{item.title}}</router-link></div>\n        <div style=\"width: 150px; float: left;\" v-text=\"item.name\"></div>\n        <div style=\"width: 150px; float: left;\" v-text=\"item.regdate\"></div>\n        <hr style=\"width: 600px\">\n      </div>\n      <form id=\"searchForm\" ref=\"searchForm\">\n        <input type=\"hidden\" id=\"page\" name=\"page\" value=\"1\">\n        <select name=\"searchKey\" v-model=\"searchKey\">\n          <option value=\"title\">제목</option>\n          <option value=\"name\">작성자</option>\n        </select>  \n        <input type=\"text\" name=\"searchValue\" v-model=\"searchValue\">\n        <button type=\"button\" @click=\"search\" id=\"searchBtn\" class=\"btn-search\"> 검색</button>\n      </form>\n      <div>\n        \n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport axios from \"axios\";\n//import PaginationComponent from './layout/PaginationComponent.vue';\n// import {plusCalculator} from './common.js'; //reactive 추가\nexport default {\n  //components: { PaginationComponent },\n  name: 'mainComponent',\n  data () {\n    return {\n      list : [],\n      searchKey : '',\n      searchValue: ''\n    }\n  },\n  methods:{\n    async search(){\n\n      try { \n        var searchKey = this.$refs.searchForm.searchKey.value\n        var searchValue = this.$refs.searchForm.searchValue.value\n\n        if (searchValue == \"\") {\n          searchValue = null;\n          searchKey = null;\n        }\n        // console.log(\"searchKey...?\"+searchKey);\n        // console.log(\"searchValue...?\"+searchValue);\n        const res =  await axios.get(\"http://localhost:6005/freeBoardList/\"+searchKey+\"/\"+searchValue);\n        //console.log(res)\n\n        this.list = res.data.list;\n      } catch (error) {\n        console.log(error);\n      }\n    }\n\n  },\n  mounted() {\n    //console.log(\"mount 되고나서 search()함수를 탄다\")\n    console.log(this.$route)\n    this.search();\n  }\n}\n</script>\n\n<style scoped>\n</style>\n"],"mappings":"AAkCA,OAAOA,KAAI,MAAO,OAAO;AACzB;AACA;AACA,eAAe;EACb;EACAC,IAAI,EAAE,eAAe;EACrBC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,IAAG,EAAI,EAAE;MACTC,SAAQ,EAAI,EAAE;MACdC,WAAW,EAAE;IACf;EACF,CAAC;EACDC,OAAO,EAAC;IACN,MAAMC,MAAMA,CAAA,EAAE;MAEZ,IAAI;QACF,IAAIH,SAAQ,GAAI,IAAI,CAACI,KAAK,CAACC,UAAU,CAACL,SAAS,CAACM,KAAI;QACpD,IAAIL,WAAU,GAAI,IAAI,CAACG,KAAK,CAACC,UAAU,CAACJ,WAAW,CAACK,KAAI;QAExD,IAAIL,WAAU,IAAK,EAAE,EAAE;UACrBA,WAAU,GAAI,IAAI;UAClBD,SAAQ,GAAI,IAAI;QAClB;QACA;QACA;QACA,MAAMO,GAAE,GAAK,MAAMX,KAAK,CAACY,GAAG,CAAC,sCAAsC,GAACR,SAAS,GAAC,GAAG,GAACC,WAAW,CAAC;QAC9F;;QAEA,IAAI,CAACF,IAAG,GAAIQ,GAAG,CAACT,IAAI,CAACC,IAAI;MAC3B,EAAE,OAAOU,KAAK,EAAE;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB;IACF;EAEF,CAAC;EACDG,OAAOA,CAAA,EAAG;IACR;IACAF,OAAO,CAACC,GAAG,CAAC,IAAI,CAACE,MAAM;IACvB,IAAI,CAACV,MAAM,CAAC,CAAC;EACf;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}